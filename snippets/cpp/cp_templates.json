            "\t}",
            "\tint find(int v) {",
            "\t\tif (pa[v] == v) {",
            "\t\t\treturn v;",
            "\t\t}",
            "\t\treturn pa[v] = find(pa[v]);",
            "\t}",
            "\tvoid merge(int u, int v) {",
            "\t\tu = find(u);",
            "\t\tv = find(v);",
            "\t\tif (u == v) {",
            "\t\t\treturn;",
            "\t\t}",
            "\t\tif (sz[u] < sz[v]) {",
