            "\t\t\tres[i] =",
            "\t\t\t\t(hash[i][r] - hash[i][l - 1] * pw[i][r - l + 1] % MOD + MOD) %",
            "\t\t\t\tMOD;",
            "\t\t}",
            "\t\treturn res;",
            "\t}",
            "};"
        ]
